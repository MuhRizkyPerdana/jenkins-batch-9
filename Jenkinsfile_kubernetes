pipeline {
    agent any
    stages {
        stage ('build') {
            agent {
                label 'fire'
            }
            steps {
                echo "Build App"
                sh 'docker build -t gcr.io/studi-devops-370205/studidevsecops/goapp:1 .'
            }
        }
        stage ('Push to GCR') {
            steps {
                echo "docker push gcr.io/studi-devops-370205/studidevsecops:1"
            }
        }
    }
}
