pipeline {
    agent {
        label 'fire'
    }
    stages {
        stage ('build') {
            steps {
                echo "Build App"
                sh 'docker build -t gcr.io/studi-devops-370205/studidevsecops:1 .'
            }
        }
        stage ('Push to GCR') {
            steps {
                echo "push to gcr"
                sh 'docker push gcr.io/studi-devops-370205/studidevsecops:1'
            }
        }
    }
}
